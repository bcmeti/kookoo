<!DOCTYPE html>
<html>
<head>
  <title>Kookoo</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if user_signed_in? %>
  <nav id="session-actions">
    Signed in as <%= current_user.email %> <%= link_to "Sign Out", destroy_user_session_path, :method => :delete %>
  </nav>
  <% end %>

<%= yield %>

</body>
</html>
